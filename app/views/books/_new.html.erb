<div class = "book-new-content">
 <h1>New book</h1>
 <%= form_with model: book, url: book_path,  method: :post, local:true do |f| %>
 <%= render 'layouts/error_messages', book: book %>

  <h5>Title</h5>
   <%= f.text_field :title %>

  <h5>Body</h5>
   <%= f.text_area :body %>

 <span class = "book-btn">
  <%= f.submit 'Create Book' %>
 </span>
 <% end %>
</div>
